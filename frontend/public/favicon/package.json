{
  "name": "zeropass-favicons",
  "version": "1.0.0",
  "description": "Favicon assets and generator for ZeroPass Firewall Simulator",
  "scripts": {
    "generate": "node generate-favicons.js",
    "install-deps": "npm install sharp --save-dev",
    "clean": "rm -f *.png *.ico",
    "build": "npm run install-deps && npm run generate"
  },
  "devDependencies": {
    "sharp": "^0.33.5"
  },
  "keywords": [
    "favicon",
    "icons",
    "pwa",
    "zeropass",
    "firewall"
  ],
  "author": "ZeroPass Team",
  "license": "MIT",
  "files": [
    "*.svg",
    "*.png",
    "*.ico",
    "*.webmanifest",
    "*.xml",
    "README.md"
  ]
}
